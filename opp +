#include <iostream>
using namespace std;
int main()
{
    int x, y, m = 1;
    int count = 0;
    cout<<"enter the hex number:";
    cin>>hex>>x;
    while (x != 0)
    {
        y = x & m;
        if (y == 1)
            count++;
        x = x >> 1;
    }
    cout<<"Number 1 repeated:"<<count;
    return 0;
}
